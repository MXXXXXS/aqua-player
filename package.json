{
  "name": "aqua-player",
  "version": "1.0.0",
  "description": "仿win10 Groove音乐播放器",
  "main": "app/main.js",
  "scripts": {
    "start": "electron .",
    "watch": "nodemon -w ./app --exec npm -- run start",
