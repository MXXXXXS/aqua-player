  "license": "MIT",
  "dependencies": {
    "ffmpeg-static": "^2.5.0",
    "music-metadata": "^4.2.1",
    "node-vibrant": "^3.1.3",
    "pinyin": "^2.9.0",
    "sortablejs": "^1.10.0-rc3"
  },
  "devDependencies": {
    "electron": "^v6.0.2",
    "eslint": "^6.1.0",
    "nodemon": "^1.19.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MXXXXXS/aqua-player.git"
  }
